<div id="<%= dom_id event %>">
  <p>
    <strong>Project:</strong>
    <%= event.project_id %>
  </p>

  <p>
    <strong>Start time:</strong>
    <%= event.start_time %>
  </p>

  <p>
    <strong>End time:</strong>
    <%= event.end_time %>
  </p>

  <p>
    <strong>Name:</strong>
    <%= event.name %>
  </p>

  <p>
    <strong>Notes:</strong>
    <%= event.notes %>
  </p>

  <p>
    <strong>Task:</strong>
    <%= event.task_id %>
  </p>

</div>
