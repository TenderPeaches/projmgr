<%= tag.table class: "list list--expenses", id: "expenses" do  %>
  <%= tag.thead do %>
    <%= tag.tr do %>
      <%= tag.th t('activerecord.attributes.expenses.name') %>
      <%= tag.th t('activerecord.attributes.expenses.type') %>
      <%= tag.th t('activerecord.attributes.expenses.amount') %>
      <%= tag.th t('activerecord.attributes.expenses.incurred_dt') %>
      <%= tag.th %>
    <% end %>
  <% end %>
  <%= tag.tbody id: "expenses-list" do %>
    <% expenses.each do |expense| %>
        <%= render partial: "expenses/row", locals: { expense: expense } %>
    <% end %>
  <% end %>
<% end %>