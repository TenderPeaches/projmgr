<div id="<%= dom_id shift %>">
  <p>
    <strong>Duration: </strong>
    <%= shift.duration %>
  </p>
  <p>
    <strong>Start:</strong>
    <%= shift.start %>
  </p>

  <p>
    <strong>End:</strong>
    <%= shift.end %>
  </p>

  <p>
    <strong>Notes:</strong>
    <%= shift.notes %>
  </p>

  <p>
    <strong>Task:</strong>
    <%= shift.task_id %>
  </p>

</div>
